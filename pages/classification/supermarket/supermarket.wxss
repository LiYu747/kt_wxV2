.tabbar {
  width: 100%;
  height: 88rpx;
  background: #FFFFFF;
  color: #666;
  font-size: 30rpx;
  border-bottom: 1rpx solid #D2D2D2;
  position: fixed;
  z-index: 9;
}

.top {
  height: 100rpx;
}

.item {
  height: 88rpx;
}

.dv {
  color: #F07535;
  height: 84rpx; 
  border-bottom: 1px solid #F07535;
  border-top: 1px solid #FFFFFF;
}

.itemimg {
  width: 144rpx;
  height: 144rpx;
  margin-left: 20rpx;
  margin-top: 20rpx;
}

.wujiaoimg {
  width: 20rpx;
  height: 20rpx;
}

.jianbimg {
  width: 120rpx;
  height: 38rpx;
}

.itembox {
  width: 690rpx;
    padding-bottom: 60rpx; 
  background: #FFFFFF;
  border-radius: 10rpx;
  margin-top: 30rpx;
  box-shadow: 0px 4px 4px 0px rgba(9, 9, 9, 0.1);
}

.name {
  color: #333333;
  font-size: 32rpx;
}

.score {
  color: #FFA800;
  font-size: 24rpx;
  margin: 10rpx 0;
}

.timer {
  color: #9A9A9A;
  right: 20rpx;
}

.details {
  color: #999999;
  font-size: 24rpx;
}

.databox {
  width: 425rpx;
  height: 64rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.Hours {
  width: 120rpx;
  height: 38rpx;
  color: #FFFFFF;
  font-size: 22rpx;
}

.Views {
  color: #999999;
  font-size: 22rpx;
  right: 20rpx;
}

.nextimg {
  width: 10rpx;
  height: 20rpx;
  margin-left: 15rpx;
}

.line {
  height: 30rpx;
}

.itemsimg {
  width: 190rpx;
  height: 130rpx;
  margin-top: 10rpx;
  margin-right: 10rpx;
}

.simulation{
  display: flex;
  justify-content: center;
  font-size: 30rpx;
  color: #b3b3b3;
}